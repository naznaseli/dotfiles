// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    //! ターミナルフォーカス
    {   "key": "ctrl+0",               "command": "workbench.action.terminal.toggleTerminal"},
    //! タブ移動(右)
    {   "key": "ctrl+tab",              "command": "workbench.action.nextEditor"},
    //! タブ移動(左)
    {   "key": "ctrl+shift+tab",        "command": "workbench.action.previousEditor"},
    //! f7ビルド
    {   "key": "f7",                    "command": "workbench.action.tasks.build"},
    //! 検索ウィジェットにフォーカス無効
    //{   "key": "ctrl+f",                "command": "workbench.action.terminal.focusFindWidget"},
    //! 検索ウィジェットにフォーカス無効(統合ターミナルフォーカス時)
    {   "key": "ctrl+f",                "command": "workbench.action.terminal.focusFindWidget",        "when": "terminalFocus"},
]
